{
  "name": "moodle-mcp",
  "version": "1.0.0",
  "description": "MCP server for Moodle LMS integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "moodle-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "npm run build && node dist/test.js",
    "test:dev": "node --loader ts-node/esm src/test.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "moodle",
    "lms",
    "education"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
